#include <stdio.h>

int main() {
    int a,b,swap;
    printf("enter value of a");
    scanf("%d",&a);
    printf("enter value of b");
    scanf("%d",&b);
    swap=a;
    a=b;
    b=swap;
    printf("after swapping");
    printf("the value of a is %d",a);
    printf("the value of b is %d",b);


    return 0;
}