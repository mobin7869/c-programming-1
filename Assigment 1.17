#include <stdio.h>

int main() {
    float side, area, perimeter;

    printf("Enter the length of side of square: ");
    scanf("%f", &side);

    area = side * side;            
    perimeter = 4 * side;         

    printf("Area of square = %.2f", area);
    printf("Perimeter of square = %.2f", perimeter);

    return 0;
}